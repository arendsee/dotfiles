priority 0

snippet href "Wrap a selection in a url" w
<a href=${1:URL}>${VISUAL}</a>
endsnippet

snippet default "Make header and body" b
<!DOCTYPE HTML>
<html lang=en-US>
	<head>
		<title>${1:web title}</title>
		<meta name="description" content="${2:short description}">
		<meta charset=UTF-8>
	</head>
	<body>
		${0}
	</body>
</html>
endsnippet

snippet teximport "In the header, import the scripts needed to interpet Tex equations" b
<script type="text/x-mathjax-config">
	MathJax.Hub.Config({
		extensions: ["tex2jax.js"],
		jax: ["input/TeX","output/HTML-CSS"],
		tex2jax: {inlineMath: [["$","$"],["\\(","\\)"]]}
	});
</script>
<script type="text/javascript" src="http://cdn.mathjax.org/mathjax/2.2-latest/MathJax.js?config=TeX-AMS_HTML"></script>
endsnippet

snippet td "expand td tag" b
<td>
	${VISUAL}
</td>
endsnippet

snippet p "expand paragraph" b
<p> ${VISUAL} </p>
endsnippet

